{
  "name": "SC_1",

  "states": {
    "initial": "initial",

    "created": "created",

    "in_effect": "in_effect",

    "suspended": "suspended",

    "active": "active",

    "unassigned": "unassigned",

    "successful_termination": "successful_termination",

    "unsuccessful_termination": "unsuccessful_termination"
  },

  "transitions": {
    "create_contract": {
      "source": "initial",
      "target": "created",
      "event": "create",
      // todos que nao possuem triggers
      "obligations_created": [
        "obligation 1",
        "obligation 2",
        "obligation N"
      ],
      "powers_created": ["power 1", "power 2", "power N"]
    },

    "activate_contract": {
      "source": "created",
      "target": "in_effect",
      "event": "activate",
      // contract only becomes active on its effective date.
      // todos com antecedente === true
      "obligations_activated": [
        "obligation 1",
        "obligation 2",
        "obligation N"
      ],
      "powers_activated": ["power 1", "power 2", "power N"]
    },

    "activate_obligations": {
      "source": "active",
      "target": "active",
      "event": "activate obligation",
      "obligations_activated": [
        ["trigger 1", "obligation 1"],
        ["trigger 2", "obligation 2"]
      ]
    },

    "activate_powers": {
      "source": "active",
      "target": "active",
      "event": "activate power",
      "powers_activated": [
        ["trigger 1", "power 1"],
        ["trigger 2", "power 2"]
      ]
    },
//     A contract may
// also be suspended if one of the parties exerts its suspension powers, or if a force
// majeure occurs, e.g., a natural catastrophe. Upon suspension, all obligation and
// power instances associated with the suspended contract are suspended as well.

    "suspend_contract": {
      "source": "in_effect",
      "target": "suspended",
      "event": "suspend",
      // power com consequent === cSUSPENSION(...)
      "by_powers": ["power 1", "power 2", "power N"]
      
    },

    // The suspended contract waits for an event that resumes it, such as a suspension
// deadline or an action performed by some party.
    "resume_contract": {
      "source": "suspended",
      "target": "in_effect",
      "event": "resume",
      // Só powers e algo do tipo cRESUME ?
      "by_powers": ["power 1", "power 2"],
      "obligations_to_be_resumed": ["obligation 1", "obligation 2"],
      "powers_to_be_resumed": ["power 1", "power 2"]
    },

//     a contract may enter the Unassign state when the assigner withdraws,
// and then will remain in that state until the assignee is assigned

    "replace_party": {
      "source": "in_effect",
      "target": "unassigned",
      "event": "revoke party",
      "by_powers": ["power 1", "power 2", "power N"]
    },


//     A contract
// successfully terminates (SuccessfulTermination) if all of its active obligations
// except surviving ones are fulfilled
    "fulfill_active_obligations": {
      "source": "in_effect",
      "target": "successful_termination",
      "event": "fulfill active obligations and powers",
      //poderes não precisam ser cumpridos (acho)
      //obligations in effect
      // 1o -> obligations sem trigger
      //verificar oVIOLATION para montar outros sets
      "obligations_fulfilled": [
        ["obligation 1", "obligation 2", "power 1", "power 2"],
        ["obligation 3", "obligation 4", "power 3", "power 4"]
      ]
    },

    // In other cases, namely termination due to
    // the exertion of a power or contract expiration while in the Active superstate,
    // the contract and its active obligations and powers terminate unsuccessfully (Un-
    // successfulTermination).

    "terminate_contract": {
      "source": "active",
      "target": "unsuccessful_termination",
      "event": "terminate",
      "obligations_unfulfilled": [
        ["not obligation 1", "not obligation 2", "power 1", "power 2"],
        ["not obligation 3", "not obligation 4", "power 3", "power 4"]
      ],
      //cUNSUCCESSFUL_TERMINATION
      "by_powers": ["power 1", "power 2", "power N"]
    }

  
  }
}
